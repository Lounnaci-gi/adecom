# Chemin vers le dossier contenant les fichiers DBF et NTX
DBF_FOLDER_PATH=./data/dbf

# Configuration de la base de données dBASE
DBF_DATABASE_NAME=database.dbf
DBF_INDEX_FILE=index.ntx

# Paramètres de connexion
DBF_ENCODING=latin1
DBF_USE_MEMO_FILES=true

# Port du serveur (optionnel, par défaut 3000)
PORT=3000